public class Solution {
    public int[] TwoSum(int[] nums, int target) {
        int firstNum = 0;
        while (firstNum <= nums.Length - 1)
        {
            for (int i = firstNum + 1; i < nums.Length; i++)
            {
                if (nums[firstNum] + nums[i] == target){

                    return new int[2] {firstNum, i};
                }
            }
            firstNum++;
        }
        return new int[0];
    }
}